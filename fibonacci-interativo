#include <stdio.h>

int main (){
    int i, tot;
    long long int num[3]={1, 0, 0};

    printf("-- Digite o tamanho da s√©rie: ");
    scanf("%d", &tot);
    printf("\n============================\n");
    for (i=0; i<tot; i++){
        num[2] = num[0]+num[1];
        printf("%lld ", num[2]);
        num[0] = num[1];
        num[1] = num[2];
    }
    printf("\n");
    return 0;
}
