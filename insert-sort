void insertionSort1(int n, int arr_count, int* arr) {
    int i, j, last;
    last = arr[n-1];
    i = n-2;
    while (i>=0 && arr[i]>last){
        arr[i+1] = arr[i];
        for (j=0; j<n; j++) printf("%d ", arr[j]);
        printf("\n");    
        i--;
    }
    if (arr[i]<last){
        arr[i+1] = last;
        for (j=0; j<n; j++) printf("%d ", arr[j]);
        printf("\n");
    }

}
